<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.mao.model.UserConversionEntity">
        <table name="user_conversion" schema="Project"/>
        <id-class class="com.mao.model.UserConversionEntityPK"/>
        <attributes>
            <id name="fuserId">
                <column name="fuserId"/>
            </id>
            <id name="tuserId">
                <column name="tuserId"/>
            </id>
            <basic name="fuserState">
                <column name="fuserState"/>
            </basic>
            <basic name="tuserState">
                <column name="tuserState"/>
            </basic>
            <basic name="createTime">
                <column name="createTime"/>
            </basic>
            <basic name="messageFlag">
                <column name="messageFlag"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
